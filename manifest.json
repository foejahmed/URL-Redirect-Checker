{
  "manifest_version": 3,
  "name": "URL Redirect Checker",
  "version": "1.4",
  "host_permissions": ["<all_urls>"],
  "permissions": ["tabs", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
    "icons": {
     "16": "images/icon16.png",
     "48": "images/icon48.png",
     "128": "images/icon128.png"
 }
}
